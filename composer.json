{
    "name": "tempocerto/agendarentrega-php",
    "description": "Client para comunicação com a API do AgendarEntrega.com",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Oliver Kraemer",
            "email": "oliverkra@gmail.com"
        }
    ],
    "require": {
      "php": "^5.3 || ^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^4"
    },
    "autoload": {
        "psr-4": {
            "AgendarEntrega\\": [
                "src"
            ],
            "AgendarEntregaTest\\": [
                "tests"
            ]
        }
    },
    "scripts": {
        "test": "./vendor/bin/phpunit --bootstrap vendor/autoload.php tests"
    },
    "config": {
        "optimize-autoloader": true
    }
}
